{
  "name": "chwnam/p3s",
  "description": "PhpStorm Setup Script",
  "autoload": {
    "psr-4": {
      "Chwnam\\Saops\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chwnam\\Saops\\Tests\\": "tests/"
    },
    "files": [
      "tests/fixtures/common.php"
    ]
  },
  "require": {
    "php": "^8.2",
    "servo/fluidxml": "^2.0",
    "ext-dom": "*"
  },
  "require-dev": {
    "pestphp/pest": "^3.8"
  },
  "license": "MIT",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
